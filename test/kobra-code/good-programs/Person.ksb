$ blueprint Person (name, age, hairColor, exercise)
    has {
        name = name,
        age = age,
        hairColor = hairColor # "black" 
    }
    does {
        do_exercise = exercise # running,
        running = proc ():
            say("26.2 miles")
        end
    }
    synget {
        name, age, hairColor
    }
    synset {
        hairColor
    }
    defcc